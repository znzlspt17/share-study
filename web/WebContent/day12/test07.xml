<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE 학생들 [
	<!ELEMENT 학생들 ( 학생)+ >
	<!ELEMENT 학생 (학번, 이름, 나이*, 주소, 전화번호?)>
	<!ELEMENT 학번 (#PCDATA)>
	<!ELEMENT 이름 (#PCDATA)>
	<!ELEMENT 나이 (#PCDATA)>
	<!ELEMENT 주소 (#PCDATA)>
	<!ELEMENT 전화번호 (#PCDATA)>

	<!ATTLIST 학생 학년 (1|2|3|4|5|6) "1">
	<!ATTLIST 학번 학과 (컴공|건축설계과|의예과) "컴공">
	<!--
	사용횟수
	 + : 1개 이상
	 * : 0개 이상
	 ? : 0 OR 1 
	 A|B : A 또는 B 둘 중에 하나
	 () : 요소를 그룹으로 선언해서 사용
	-->
]>

<학생들>
	<학생 학년="1">
		<학번 학과="컴공"> 123</학번>
		<이름>김개똥</이름>
		<나이>33</나이>
		<주소>고양시</주소>
	</학생>

	<학생 학년="5">
		<학번 학과="건축설계과">433</학번>
		<이름>김아무개</이름>
		<나이>33</나이>
		<나이>55</나이>
		<주소>고양시</주소>
		<전화번호>111</전화번호>
	</학생>

	<학생 학년="6">
		<학번 학과="의예과">4323</학번>
		<이름>김미미</이름>
		<주소>서울특별시</주소>
	</학생>
</학생들>
